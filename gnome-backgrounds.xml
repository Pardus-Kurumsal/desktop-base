<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE wallpapers SYSTEM "gnome-wp-list.dtd">
<wallpapers>
  <wallpaper>
    <!-- don't include words such as "wallpaper" (avoids translations) -->
    <name>Gray</name>
    <filename>/usr/share/images/desktop-base/gray.xml</filename>
    <options>zoom</options>
    <pcolor>#ffffff</pcolor>
    <scolor>#000000</scolor>
  </wallpaper>
  <wallpaper>
    <name>Blue lock screen</name>
    <filename>/usr/share/images/desktop-base/blue-lockscreen.xml</filename>
    <options>zoom</options>
    <pcolor>#ffffff</pcolor>
    <scolor>#000000</scolor>
  </wallpaper>
  <wallpaper>
    <name>Anger</name>
    <filename>/usr/share/images/desktop-base/anger.xml</filename>
    <options>zoom</options>
    <pcolor>#ffffff</pcolor>
    <scolor>#000000</scolor>
  </wallpaper> 
</wallpapers>
